{
  "schemaVersion": 1,
  "id": "${modid}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${author}"
  ],
  "contact": {
    "homepage": "${homepage}",
    "sources": "${github}"
  },
  "license": "${license}",
  "icon": "${icon}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "fr.frinn.custommachinery.fabric.CustomMachineryFabric"
    ],
    "modmenu": [
      "fr.frinn.custommachinery.fabric.integration.modmenu.ModMenuIntegration"
    ],
    "jei_mod_plugin": [
      "fr.frinn.custommachinery.client.integration.jei.CustomMachineryJEIPlugin"
    ],
    "jade": [
      "fr.frinn.custommachinery.fabric.integration.jade.CMWailaPlugin"
    ]
  },
  "mixins": [
    "custommachinery.fabric.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=${minecraft_version}",
    "architectury": ">=${architectury_version}",
    "cloth-config": ">=${cloth_config_version}",
    "java": ">=17"
  }
}